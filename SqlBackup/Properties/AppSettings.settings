<?xml version='1.0' encoding='utf-8'?>
<SettingsFile xmlns="http://schemas.microsoft.com/VisualStudio/2004/01/settings" CurrentProfile="(Default)" GeneratedClassNamespace="SqlBackup.Properties" GeneratedClassName="AppSettings">
  <Profiles />
  <Settings>
    <Setting Name="SQLBackupCommand" Type="System.String" Scope="User">
      <Value Profile="(Default)">BACKUP DATABASE [{0}] TO DISK='{1}'</Value>
    </Setting>
